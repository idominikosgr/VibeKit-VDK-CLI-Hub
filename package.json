  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@typescript/native-preview": "7.0.0-dev.20250701.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5"
  },
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=10.12.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not android 4.4.3"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,css,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.sql": [
      "prettier --write --parser sql"
    ],
    "package.json": [
      "prettier --write"
    ],
    "src/types/database.types.ts": [
      "bash -c 'pnpm run types:verify || true'"
    ],
    "supabase/migrations/*.sql": [
      "bash -c 'echo \"üóÑÔ∏è Validating migration file: $0\" && supabase db lint || true'"
    ],
    "*.{ts,tsx}": [
      "bash -c 'pnpm run typecheck'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "test",
          "chore",
          "perf",
          "ci",
          "build",
          "db",
          "api",
          "ui",
          "auth",
          "config"
        ]
      ],
      "subject-max-length": [
        2,
        "always",
        72
      ],
      "body-max-line-length": [
        2,
        "always",
        100
      ]
    }
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^server-only$": "<rootDir>/node_modules/server-only/index.js"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(@vercel/analytics|@vercel/speed-insights)/)"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/.next/",
    "<rootDir>/node_modules/"
  ],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,ts,tsx}",
    "!src/**/*.d.ts",
    "!src/**/*.stories.{js,jsx,ts,tsx}",
    "!src/**/*.test.{js,jsx,ts,tsx}",
    "!src/**/index.{js,jsx,ts,tsx}"
  ],
  "packageManager": "pnpm@10.12.4",
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build",
          "typecheck",
          "lint:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "build:turbopack": {
        "dependsOn": [
          "^build:turbopack",
          "typecheck",
          "lint:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "build:check": {
        "dependsOn": [
          "^build:check"
        ],
        "outputs": [
          ".next/**",
          "!.next/cache/**"
        ]
      },
      "lint": {
        "dependsOn": [
          "^lint"
        ]
      },
      "lint:check": {
        "dependsOn": [
          "^lint:check"
        ]
      },
      "typecheck": {
        "dependsOn": [
          "^typecheck"
        ]
      },
      "test": {
        "dependsOn": [
          "^test"
        ]
      },
      "test:ci": {
        "dependsOn": [
          "^test:ci"
        ]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "dev:turbopack": {
        "cache": false,
        "persistent": true
      },
      "validate": {
        "dependsOn": [
          "typecheck",
          "lint:check",
          "format:check"
        ]
      },
      "validate:full": {
        "dependsOn": [
          "validate",
          "test:ci",
          "build:check"
        ]
      },
      "perf:lighthouse": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "pnpm": {
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    },
    "patchedDependencies": {},
    "allowedDeprecatedVersions": {
      "inflight": "*",
      "glob": "*"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "core-js",
      "core-js-pure",
      "esbuild",
      "msw",
      "protobufjs",
      "puppeteer",
      "sharp",
      "supabase",
      "tesseract.js",
      "unrs-resolver"
    ]
  }
}
